local function FWYOCIL_fake_script() -- frame.LocalScript 
	local script = Instance.new('LocalScript', frame)

	local frame = script.Parent
	
	frame.HUBDESC.Visible = false
	frame.HUBTITLE.Visible = false
	frame.Rphoto.Visible = false
	frame.welcometext.Visible = true
	frame.linha1.Visible = false
	frame.Xexit.Visible = false
	frame.noclip.Visible = false
	frame.welcometext.TextTransparency = 1
	wait(0.1)
	frame.welcometext.TextTransparency = 0.9
	wait(0.1)
	frame.welcometext.TextTransparency = 0.8
	wait(0.1)
	frame.welcometext.TextTransparency = 0.7
	wait(0.1)
	frame.welcometext.TextTransparency = 0.6
	wait(0.1)
	frame.welcometext.TextTransparency = 0.5
	wait(0.1)
	frame.welcometext.TextTransparency = 0.4
	wait(0.1)
	frame.welcometext.TextTransparency = 0.3
	wait(0.1)
	frame.welcometext.TextTransparency = 0.2
	wait(0.1)
	frame.welcometext.TextTransparency = 0.1
	wait(0.1)
	frame.welcometext.TextTransparency = 0
	wait(1)
	frame.welcometext.TextTransparency = 0.1
	wait(0.1)
	frame.welcometext.TextTransparency = 0.2
	wait(0.1)
	frame.welcometext.TextTransparency = 0.3
	wait(0.1)
	frame.welcometext.TextTransparency = 0.4
	wait(0.1)
	frame.welcometext.TextTransparency = 0.5
	wait(0.1)
	frame.welcometext.TextTransparency = 0.6
	wait(0.1)
	frame.welcometext.TextTransparency = 0.7
	wait(0.1)
	frame.welcometext.TextTransparency = 0.8
	wait(0.1)
	frame.welcometext.TextTransparency = 0.9
	wait(0.1)
	frame.welcometext.TextTransparency = 1
	wait(1)
	frame.HUBDESC.Visible = true
	frame.HUBTITLE.Visible = true
	frame.Rphoto.Visible = true
	frame.welcometext.Visible = false
	frame.linha1.Visible = true
	frame.Xexit.Visible = true
	frame.noclip.Visible = true
end
coroutine.wrap(FWYOCIL_fake_script)()
local function EAWSDD_fake_script() -- noclip.LocalScript 
	local script = Instance.new('LocalScript', noclip)

	local button = script.Parent
	local frame = button.Parent
	
	button.MouseButton1Click:Connect(function()
		frame.noclippage.ativar.Visible = true
		frame.noclippage.ativartext.Visible = true
		frame.noclippage.desativar.Visible = true
	end)
end
coroutine.wrap(EAWSDD_fake_script)()
local function BXABISJ_fake_script() -- noclippage.LocalScript 
	local script = Instance.new('LocalScript', noclippage)

	local player = game.Players.LocalPlayer
	local ativar = script.Parent:WaitForChild("ativar")
	local desativar = script.Parent:WaitForChild("desativar")
	
	local noclipOn = false
	
	local function setNoclip(enable)
		local character = player.Character or player.CharacterAdded:Wait()
		noclipOn = enable
		for _, part in ipairs(character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.CanCollide = not enable
			end
		end
	end
	
	ativar.MouseButton1Click:Connect(function()
		setNoclip(true)
	end)
	
	desativar.MouseButton1Click:Connect(function()
		setNoclip(false)
	end)
	
	game:GetService("RunService").Stepped:Connect(function()
		if noclipOn then
			local character = player.Character
			if character then
				for _, part in ipairs(character:GetDescendants()) do
					if part:IsA("BasePart") then
						part.CanCollide = false
					end
				end
			end
		end
	end)
	
end
coroutine.wrap(BXABISJ_fake_script)()
local function LQQEN_fake_script() -- Xexit.LocalScript 
	local script = Instance.new('LocalScript', Xexit)

	local button = script.Parent
	local frame = button.Parent
	
	button.MouseButton1Click:Connect(function()
		frame.Visible = false
		frame.Parent.dep.Visible = true
	end)
end
coroutine.wrap(LQQEN_fake_script)()
local function SKQY_fake_script() -- dep.LocalScript 
	local script = Instance.new('LocalScript', dep)

	local depp = script.Parent
	local frame = depp.Parent.frame
	
	depp.MouseButton1Click:Connect(function()
		frame.Visible = true
		depp.Visible = false
	end)
end
coroutine.wrap(SKQY_fake_script)()
